# Example terraform.tfvars file for staging environment
# Copy to terraform.tfvars and fill in the values

# Project settings
project_id     = "healthtrackplus-staging"
project_prefix = "healthtrackplus"
region         = "us-central1"

# Network configuration
private_subnet_cidr = "10.0.0.0/20"
public_subnet_cidr  = "10.0.16.0/24"
connector_cidr      = "10.8.0.0/28"

# Database credentials
db_name           = "healthtrackplus"
db_user           = "app"
db_password       = "change-me-123" # Change this!
db_admin_user     = "admin"
db_admin_password = "change-me-456" # Change this!

# API configuration
api_image_tag = "staging-v1.0"

# Payment integration
stripe_publishable_key = "pk_test_example"
stripe_secret_key      = "sk_test_example"
stripe_webhook_secret  = "whsec_example"

# Authentication
firebase_server_key  = "example-server-key"
google_client_id     = "example-google-client-id.apps.googleusercontent.com"
google_client_secret = "example-google-client-secret"
apple_client_id      = "example.apple.client.id"
apple_client_secret  = "example-apple-client-secret"

# Domain name
domain_name = "staging.healthtrackplus.com"

# Alerting
alert_email       = "staging-alerts@healthtrackplus.com"
slack_webhook_url = "https://hooks.slack.com/services/example"
slack_channel     = "#staging-alerts"

# Functions version
functions_version = "1.0"

# AI model configuration
model_artifact_uri = "gs://healthtrackplus-models/medical-analysis-v1"
model_container_image = "us-docker.pkg.dev/vertex-ai/prediction/tf2-cpu.2-8:latest"
gemini_training_data_uri = "gs://healthtrackplus-ai-data/training-data"
gemini_eval_data_uri = "gs://healthtrackplus-ai-data/eval-data"
gemini_validation_data_uri = "gs://healthtrackplus-ai-data/validation-data"